package com.tyss.jpahibernate.manytomany;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Data
@Entity
@Table(name="Course")
public class Course {

	@Id
	@Column
	private int cid;
	
	@Column
	private String cname;
	
	
}
