package com.tyss.jpahibernate.onetoone;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="person")
public class Person {
	@Id
	@Column(name="pid")
	private int pid;
	
	@Column(name="pname")
	private String name;
	
	


	
	

}
